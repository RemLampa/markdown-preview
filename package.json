{
  "name": "markdown-preview",
  "version": "0.1.0",
  "description": "A Simple React Markdown Preview App",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "build": "webpack -p --config webpack.config.prod.js --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "deploy": "npm run build && gh-pages --dist public",
    "test:once": "NODE_PATH=./src mocha --compilers js:babel-register -r jsdom-global/register -r ignore-styles --recursive ./test",
    "test": "npm run test:once -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ibleedfilm/markdown-preview.git"
  },
  "keywords": [
    "free code camp",
    "react",
    "webpack",
    "github pages",
    "sass",
    "markdown"
  ],
  "author": "Rem Lampa <rem@lansangan.com> (http://www.remlampa.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ibleedfilm/markdown-preview/issues"
  },
  "homepage": "https://github.com/ibleedfilm/markdown-preview#readme",
  "dependencies": {
    "marked": "^0.3.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.22.0",
    "bootstrap-sass": "^3.3.7",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.26.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
